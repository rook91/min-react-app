{
  "name": "min-react-app",
  "version": "1.0.0",
  "description": "minimal react app",
  "main": "index.js",
  "scripts": {
    "analyzeReact": "npm run analyze:webpack && npm run analyze:resultsReact",
    "analyzePreact": "npm run analyze:webpack && npm run analyze:resultsPreact",
    "analyze:webpack": "webpack -p --json --progress --profile > webpack-build-log.json",
    "analyze:resultsReact": "webpack-bundle-analyzer webpack-build-log.json distReact",
    "analyze:resultsPreact": "webpack-bundle-analyzer webpack-build-log.json distPreact",
    "dist": "webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rook91/min-react-app.git"
  },
  "keywords": [
    "min",
    "react",
    "app"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rook91/min-react-app/issues"
  },
  "homepage": "https://github.com/rook91/min-react-app#readme",
  "dependencies": {
    "preact": "^8.3.1",
    "react": "16.5.0",
    "react-dom": "16.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "7.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "copy-webpack-plugin": "^4.5.2",
    "webpack": "4.17.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "3.1.0"
  }
}
